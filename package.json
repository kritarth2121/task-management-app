{
  "name": "altre-backend-prod",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "node ace serve --watch",
    "build": "node --max-old-space-size=2048 ace build --production",
    "start": "node server.js",
    "make:model": "node ace make:model",
    "make:migration": "node ace make:migration",
    "migration:status": "node ace migration:status",
    "migration:run": "node ace migration:run",
    "migration:rollback": "node ace migration:rollback",
    "db:seed": "node ace db:seed",
    "db:reset": "node ace migration:rollback --batch=0 && node ace migration:run && node ace db:seed",
    "db:refresh": "node ace migration:refresh --seed",
    "queue": "node ace bull:listen"
  },
  "devDependencies": {
    "@adonisjs/assembler": "^5.9.5",
    "@japa/preset-adonis": "^1.2.0",
    "@japa/runner": "^2.2.2",
    "@types/jsonwebtoken": "9.0.1",
    "@types/proxy-addr": "^2.0.0",
    "@types/source-map-support": "^0.5.6",
    "adonis-preset-ts": "^2.1.0",
    "pino-pretty": "^9.1.1",
    "typescript": "~4.6",
    "youch": "^3.2.2",
    "youch-terminal": "^2.1.5"
  },
  "dependencies": {
    "@adonisjs/core": "^5.8.0",
    "@adonisjs/lucid": "^18.3.0",
    "@adonisjs/lucid-slugify": "^2.2.1",
    "@adonisjs/redis": "7.3.4",
    "@adonisjs/repl": "^3.1.0",
    "@adonisjs/view": "^6.2.0",
    "@rocketseat/adonis-bull": "^1.0.4",
    "@types/node": "^18.13.0",
    "axios": "^1.2.3",
    "docxtemplater": "^3.33.0",
    "google-auth-library": "^9.6.3",
    "googleapis": "^127.0.0",
    "jsonwebtoken": "8.5.1",
    "lodash": "^4.17.21",
    "luxon": "^3.1.1",
    "node-cron": "^3.0.3",
    "openai": "^4.67.3",
    "pg": "^8.8.0",
    "phc-bcrypt": "^1.0.8",
    "pizzip": "^3.1.4",
    "proxy-addr": "^2.0.7",
    "reflect-metadata": "^0.1.13",
    "source-map-support": "^0.5.21"
  }
}
